interface batbridge {
	AdvSendAdvert on;
	AdvHomeAgentFlag off;
	MinRtrAdvInterval 10;
	MaxRtrAdvInterval 30;
	AdvOtherConfigFlag on;
	AdvSourceLLAddress off;
	AdvLinkMTU 1280; # Minimum MTU for v6 rfc2460#5
	#prefix <%= @ipv6_net %>::/64 {
		#AdvOnLink on;
		#AdvAutonomous on;
		#AdvRouterAddr on;
	#};
	RDNSS fddd:5d16:b5dd:0:a070:1fff:fe5f:5796 {
	};
	prefix <%= @ipv6_rnet %>::/64 {
		AdvOnLink on;
		AdvAutonomous on;
		AdvRouterAddr on;
	};
};
